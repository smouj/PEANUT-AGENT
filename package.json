{
  "name": "peanut-agent-enterprise",
  "version": "1.0.0",
  "private": true,
  "description": "Enterprise AI Agent Management Platform with Gateway-Dashboard Architecture",
  "scripts": {
    "dev": "pnpm --parallel --filter './apps/*' --filter './services/*' dev",
    "build": "pnpm --filter './packages/*' build && pnpm --filter './services/*' build && pnpm --filter './apps/*' build",
    "test": "pnpm --recursive test",
    "test:coverage": "pnpm --recursive test:coverage",
    "lint": "pnpm --recursive lint",
    "type-check": "pnpm --recursive type-check",
    "clean": "pnpm --recursive clean && find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
  },
  "devDependencies": {
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": ["better-sqlite3", "esbuild", "protobufjs", "sharp", "unrs-resolver"]
  }
}
